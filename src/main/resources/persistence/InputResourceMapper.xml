<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.lvy.jewel.persistence.InputResourceMapper">
    <resultMap id="inputResourceMap" type="org.lvy.jewel.domain.InputResource">
        <id column="" property="id"/>
        <result column="month" property="month"/>
        <result column="resource" property="resource"/>
    </resultMap>
    <select id="selectInputResources" resultMap="inputResourceMap">
        select * from input_resource
    </select>


</mapper>
